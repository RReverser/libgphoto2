# -*- Makefile-automake -*-

EXTRA_DIST                  += %reldir%/ChangeLog
EXTRA_DIST                  += %reldir%/Protocol.txt
EXTRA_DIST                  += %reldir%/ez200-lsusb.txt


EXTRA_LTLIBRARIES           += kodak_ez200.la

