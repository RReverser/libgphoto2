enigma13_la_CPPFLAGS      = $(camlib_cppflags) -DLIBNAME=enigma13
enigma13_la_DEPENDENCIES  = $(camlib_dependencies)
enigma13_la_LDFLAGS       = $(camlib_ldflags)
enigma13_la_LIBADD        = $(camlib_libadd)
